; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Arc-Pub"
config/description="Metaverso 2D MMO Social"
run/main_scene="res://scenes/login/login.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Config="*res://scripts/autoload/config.gd"
Logger="*res://scripts/autoload/logger.gd"
API="*res://scripts/autoload/api.gd"
GameData="*res://scripts/autoload/game_data.gd"
ShipsRegistry="*res://scripts/autoload/ships_registry.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
